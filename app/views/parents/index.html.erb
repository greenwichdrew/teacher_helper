<h1><%= "#{@teacher.name}'s Dashboard" %></h1>

<h2>Students</h2>
<% @students.each do |student|%>
  <p><%= link_to "#{student.name}", edit_student_path(student) %></p>
<% end %>

<h2>Parents</h2>
<% @parents.each do |parent|%>
  <p><%= link_to "#{parent.name}", edit_parent_path(parent) %></p>
<% end %>


<p><%= link_to "New Parent", new_parent_path %></p>

<p><%= link_to "New Student", new_student_path %></p>

<p><%= link_to "New Teacher", new_teacher_path %></p>

<p><%= link_to "New Grades", new_grade_path %></p>

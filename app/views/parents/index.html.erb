<%= "#{}'s Students" %>

<% @parents.each do |parent|%>
  <span><%= parent.name %></span><span><%=parent.students %></span>
<% end %>

<% @students.each do |student|%>
  <p><%= student.parent_name %></p>
<% end %>

<h3><%= link_to "New Parent", new_parent_path %></h3>

<h3><%= link_to "New Student", new_student_path %></h3>
